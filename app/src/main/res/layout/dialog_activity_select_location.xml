<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF">

        <FrameLayout
            android:id="@+id/location_search_result_container"
            android:layout_width="0dp"
            android:layout_height="500dp"
            android:background="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fragment2">


            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/fragment3"
                android:name="com.uos.upkodah.local.map.fragment.KakaoMapFragment"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:tag="location_search_map" />
        </FrameLayout>

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/fragment2"
            android:name="com.uos.upkodah.user.fragment.SearchBarFragment"
            android:layout_width="0dp"
            android:layout_height="70dp"
            android:tag="location_search_window"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>